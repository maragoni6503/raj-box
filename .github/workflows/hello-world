name: Hello World


on:
  
  push:
    branches: [ "main" ]
  pull_request:
    branches: [ "main" ]

  workflow_dispatch:
  
jobs:
  build:
    runs-on: ubuntu-latest

    steps:
    - name: Checkout code
      uses: actions/checkout@v2

    - name: Say Hello
      run: echo "Hello, ${{ env.NAME }}!"

env:
  NAME: Your Name Here
